<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="tests">
    <groups>
        <run>
            <include name="ErrorHandling"/>
            </run>
    </groups>
    <test verbose="2" preserve-order="true" thread-count="5" name="Submit Order Test">
        <classes>
            <class name="akashdhembare2000.Tests.SubmitOrderTest" />
        </classes>
    </test>

    <test thread-count="5" name="Error Validations Test">
        <classes>
            <class name="akashdhembare2000.Tests.ErrorValidationsTests"/>
        </classes>
    </test>
</suite>